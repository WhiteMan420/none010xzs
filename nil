Section01:CreateDropdown("Arena", Enemyman, function(Enemyman)  
 getgenv().enemyman = Enemyman
     end)

     Section01:CreateToggle("Auto Farm", false, function(m)         
         mm = m
         while mm do task.wait()
             for _,v in pairs(game:GetService("Players"):GetChildren()) do
                 if  v.Name ==  "yes" and v.extrastats.Gamemode.Value == "None"  then
         local args = {
             [1] = workspace.Maps.Manhattan.EnemyFolder:FindFirstChild(enemyman)
         }

         game:GetService("ReplicatedStorage").Remotes.HeroRemotes.AttackEnemy:FireServer(unpack(args))

         repeat wait() 
             game:GetService("ReplicatedStorage").Remotes.HeroRemotes.AttackClick:InvokeServer()
         until v.extrastats.Gamemode.Value == "None"
     end
 end
     end
 end)
 Section01:CreateToggle("Teleport to mob", false, function(z)         
     zz = z
     while zz do task.wait()
         for _,v in pairs(game:GetService("Players"):GetChildren()) do
             if  v.Name ==  "yes" and v.extrastats.Gamemode.Value == "None"  then
                 local Time = 0.5
                 local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Time), {CFrame = game.workspace.Maps.Manhattan.EnemyFolder[enemyman].HumanoidRootPart.CFrame})                                       
                 tween:Play()
                 tween.Completed:Wait(E)
                 wait(120)    
         wait(120)
         repeat wait() 
             game:GetService("ReplicatedStorage").Remotes.HeroRemotes.AttackClick:InvokeServer()
         until v.extrastats.Gamemode.Value == "None"
         wait(2)
     end
 end
 end
end)

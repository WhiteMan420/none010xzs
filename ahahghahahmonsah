            "Quinque/Kagune"
        }
    }
)

Misc.TextField(
    {
        Text = "Stat Amount",
        Callback = function(Value)
            getgenv().num = Value
        end
    }
)
Misc.Toggle(
    {
        Text = "Auto Upgrade Stats",
        Callback = function(Value)
            a3 = Value
            while a3 do
                task.wait()
                if stat == "Quinque/Kagune" then
                    local args = {
                        [1] = "DontSpyStupid",
                        [2] = "WeaponAddButton",
                        [3] = tonumber(num)
                    }

                    game:GetService("Players").LocalPlayer.StorageFile.statusfunction:InvokeServer(unpack(args))
                elseif stat == "Speed" then
                    local args = {
                        [1] = "DontSpyStupid",
                        [2] = "SpeedAddButton",
                        [3] = tonumber(num)
                    }

                    game:GetService("Players").LocalPlayer.StorageFile.statusfunction:InvokeServer(unpack(args))
                elseif stat == "Durability" then
                    local args = {
                        [1] = "DontSpyStupid",
                        [2] = "DurabilityAddButton",
                        [3] = tonumber(num)
                    }

                    game:GetService("Players").LocalPlayer.StorageFile.statusfunction:InvokeServer(unpack(args))
                end
            end
        end,
        Enabled = false
    }
)
Misc.Dropdown(
    {
        Text = "Select Stage",
        Callback = function(Value)
            getgenv().stage = Value
        end,
        Options = {
            "One",
            "Two",
            "Three",
            "Four"
        }
    }
)

Misc.Toggle(
    {
        Text = "Auto Requip",
        Callback = function(Value)
            a5 = Value
            while a5 do
                task.wait()
                if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
                    wait(1)
                    local args1 = {
                        [1] = "N",
                        [2] = "Down",
                        [3] = CFrame.new(1647.1573486328125, 0.38918161392211914, 384.2356872558594) *
                            CFrame.Angles(-0.4261299669742584, -0.35624513030052185, -0.15701371431350708),
                        [5] = CFrame.new(1629.7091064453125, 19.77010154724121, 426.93023681640625) *
                            CFrame.Angles(-0.2617993950843811, 0, -0)
                    }

                    game:GetService("Players").LocalPlayer.Character.Remotes.KeyEvent:FireServer(unpack(args1))
                    wait(1)
                    local args = {
                        [1] = stage,
                        [2] = "Down",
                        [3] = CFrame.new(1647.1573486328125, 0.38918161392211914, 384.2356872558594) *
                            CFrame.Angles(-0.4261299669742584, -0.35624513030052185, -0.15701371431350708),
                        [5] = CFrame.new(1629.7091064453125, 19.77010154724121, 426.93023681640625) *
                            CFrame.Angles(-0.2617993950843811, 0, -0)
                    }

                    game:GetService("Players").LocalPlayer.Character.Remotes.KeyEvent:FireServer(unpack(args))
                end
            end
        end,
        Menu = {
            Information = function(self)
                X.Banner(
                    {
                        Text = "Re equip upon death"
                    }
                )
            end
        },
        Enabled = false
    }
)

Misc.Toggle(
    {
        Text = "No Name",
        Callback = function(Value)
            bb = Value
            while bb do
                task.wait()
                pcall(
                    function()
                        for i, v in next, game.Players.LocalPlayer.Character.Head:GetChildren() do
                            if v:IsA "BillboardGui" then
                                v:Destroy()
                            end
                        end
                    end
                )
            end
        end,
        Enabled = false
    }
)

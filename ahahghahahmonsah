    Text = "Auto Farm",
    Callback = function(Value)
    aa = Value
    while aa do task.wait()
        pcall(function()
                if mob == "Low Level Aogiri" then
                lp.Character.HumanoidRootPart.CFrame = CFrame.new(getLOW().Position + Vector3.new(0,tonumber(des),0),getLOW().Position) 
                    local args = {
                        [1] = "Mouse1",
                        [2] = "Down",
                        [3] = CFrame.new(1680.3150634765625, 3.5645694732666016, 350.1279296875) * CFrame.Angles(-0.31118515133857727, -0.9888778924942017, -0.26249679923057556),
                        [5] = CFrame.new(1599.6353759765625, 19.8182373046875, 400.662109375) * CFrame.Angles(-0.42900002002716064, -0.6313362121582031, -0.2636885643005371)

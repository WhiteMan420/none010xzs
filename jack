        )

    end,
    Enabled = false
})

Main1.Toggle({
    Text = "no recoil",Settings.Recoil,
    Callback = function(d)
        Settings.Recoil = d
        for i, v in pairs(game:GetService("ReplicatedStorage").Weapons:GetDescendants()) do
            if v.Name == "MaxSpread" or v.Name == "Spread" or v.Name == "RecoilControl" then
                if Settings.Recoil then
                    v.Value = 0 
                else
                    v.Value = 1
                end
            end
        end

    end,
    Enabled = false
})
